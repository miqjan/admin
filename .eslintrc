{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
          "jsx": true
        },
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "module": true,
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "plugin:react/recommended"
    ],
    "rules": {
        "comma-dangle": [1,"never"],
        "no-dupe-args": 1,
        "no-dupe-keys": 1,
        "no-duplicate-case": 2,
        "no-empty-character-class": 1,
        "no-empty": 1,
        "no-extra-boolean-cast": 1,
        "no-var": 2,
        "require-yield": 2
    }
}
